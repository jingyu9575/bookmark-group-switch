- var repoRoot = ''
- var subtitle = false
- var noFavicon = false

doctype html
html
	head
		meta(charset='utf-8')
		meta(name='viewport' content='width=device-width, initial-scale=1.0')
		title
		block head
		if !noFavicon
			link(rel='icon' href='/icons/icon.svg')
	body(data-subtitle=subtitle)
		script(type='module' src='/common/base.js')
		block body